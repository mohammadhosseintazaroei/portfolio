{
  "name": "automoby-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --cache",
    "format": "prettier --write .",
    "test": "npm run check-format && npm run check-lint && npm run check-types",
    "test-all": "npm run test && npm run build",
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w"
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.14.3",
    "@mui/x-data-grid-premium": "^6.9.1",
    "@mui/x-date-pickers-pro": "^6.10.2",
    "@react-three/drei": "^9.83.9",
    "@react-three/fiber": "^8.14.1",
    "@types/stylis": "^4.2.0",
    "axios": "^1.4.0",
    "date-fns-jalali": "^2.13.0-0",
    "framer-motion": "^10.16.4",
    "graphql": "^16.7.1",
    "immer": "^10.0.2",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.45.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.16.0",
    "react-tilt": "^1.0.2",
    "react-vertical-timeline-component": "^3.6.0",
    "recharts": "^2.7.2",
    "stylis": "^4.3.0",
    "stylis-plugin-rtl": "^2.1.1",
    "use-immer": "^0.9.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@swc/core": "^1.3.72",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.57.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.0",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.2"
  }
}
